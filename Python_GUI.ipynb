    "#digitalclock\n",
    "\n",
    "import time\n",
    "import tkinter as tk\n",
    "def digclock(t1=''):\n",
    "    t2=time.strftime('%H:%M:%S')\n",
    "    if t1 != t2:\n",
    "        t1 = t2\n",
    "        clock.config(text=t2)\n",
    "\n",
    "    clock.after(300,digclock)\n",
    "wind = tk.Tk()\n",
    "clock = tk.Label(wind, font=('calibri',12, 'bold'),bg='blue')\n",
    "wind.title(\"Welcome to Istuti Gupta Digital Clock\")\n",
    "clock.pack()\n",
    "digclock()\n",
    "wind.mainloop()\n"
   ]
